   "source": [
    "# Create a bar chart for each variable\n",
    "chart1 = alt.Chart(train_df).mark_bar().encode(\n",
    "    x=alt.X('is_firm:N', title='Firmness'),\n",
    "    y=alt.Y('count()', title='Count'),\n",
    "    tooltip='count()'\n",
    ").properties(title='Distribution of Cheese Firmness')\n",
    "\n",
    "chart2 = alt.Chart(train_df).mark_bar().encode(\n",
    "    x=alt.X('MilkTypeEn:N', title='Milk Type'),\n",
    "    y=alt.Y('count()', title='Count'),\n",
    "    tooltip='count()'\n",
    ").properties(title='Distribution of Cheese Milk Types')\n",
    "\n",
    "chart3 = alt.Chart(train_df).mark_bar().encode(\n",
    "    x=alt.X('MilkTreatmentTypeEn:N', title='Milk Treatment'),\n",
    "    y=alt.Y('count()', title='Count'),\n",
    "    tooltip='count()'\n",
    ").properties(title='Distribution of Cheese Milk Treatments')\n",
    "\n",
    "chart4 = alt.Chart(train_df).mark_bar().encode(\n",
    "    x=alt.X('FatLevel:N', title='Fat Level'),\n",
    "    y=alt.Y('count()', title='Count'),\n",
    "    tooltip='count()'\n",
    ").properties(title='Distribution of Cheese Fat Levels')\n",
    "\n",
    "# Combine the charts into a single column using the \"vconcat\" method\n",
    "alt.hconcat(chart1, chart2, chart3, chart4)\n"
   ]
